<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0"
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
      <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>coreServlet</servlet-name>
    <servlet-class>  
			com.ray.servlet.CoreServlet
    </servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>coreServlet</servlet-name>
    <url-pattern>/coreServlet</url-pattern>
  </servlet-mapping>

<servlet>  
  <servlet-name>coreServletWaiBao</servlet-name>
    <servlet-class>  
			com.ray.servlet.CoreServlet_waiBao
    </servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>coreServletWaiBao</servlet-name>
    <url-pattern>/coreServletWaiBao</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>QRCodeServlet</servlet-name>
    <servlet-class>com.ray.servlet.QrcodeServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>QRCodeServlet</servlet-name>
    <url-pattern>/qrservlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>uploadTempMaterialServlet</servlet-name>
    <servlet-class>  
			com.ray.servlet.UploadTempMaterialServlet
    </servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>uploadTempMaterialServlet</servlet-name>
    <url-pattern>/uploadTempMaterialServlet</url-pattern>
</servlet-mapping>
<servlet>
    <servlet-name>uploadExpenseAccaoutServlet</servlet-name>
    <servlet-class>  
			com.ray.servlet.UploadExpenseAccaoutServlet
    </servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>uploadExpenseAccaoutServlet</servlet-name>
    <url-pattern>/uploadExpenseAccaoutServlet</url-pattern>
  </servlet-mapping></web-app>